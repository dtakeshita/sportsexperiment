clear; close all;
% parameters 
dt = 1e-3;%time step
t = 0:dt:5;%time vector
f = 2;%frquency of sine wave
sigma = 0.1;%standard deviation of noise
% generate a test signal
y = sin(2*pi*f*t);
% calculate time derivative
dydt = (y(3:end) - y(1:end-2))/(2*dt);%3-pont differentiation

% add noise
y_n = y + sigma*randn(size(y));%Gaussian noise added